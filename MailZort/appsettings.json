{
    "EmailSettings": {
        "PullMails": true,
        "Trash": "Deleted Messages",
        "TestMode": false,
        "SearchMode": "recent"
    },
    "rules": [
        {
            "Name": "Shipping",
            "Folder": "Inbox",
            "MoveTo": "Shipping",
            "LookIn": 3,
            "ExpressionType": 0,
            "Values": [
                "USPS"
            ]
        },
        {
            "Name": "Shipping",
            "Folder": "Inbox",
            "MoveTo": "Shipping",
            "LookIn": 1,
            "ExpressionType": 0,
            "Values": [
                "Shipped",
                "Delivered",
                "Delivery",
                "Your order",
                "Order #",
                "Order Confirmation"
            ]
        },
        {
            "Name": "Codes",
            "Folder": "Inbox",
            "MoveTo": "Trash",
            "DaysOld": 1,
            "LookIn": 0,
            "ExpressionType": 0,
            "IsEnabled": true,
            "Values": [
                "sign in code",
                "access code",
                "one time passcode",
                "verification code"
            ]
        },
        {
            "Name": "Generic",
            "Folder": "Inbox",
            "MoveTo": "Trash",
            "DaysOld": 5,
            "LookIn": 0,
            "ExpressionType": 6,
            "IsEnabled": true,
            "Values": [
                "\\b(?:free|guaranteed|win|limited\\s*time|act\\s*now)\\b",
                "\\b(?:\\$\\$\\$|cash|money|earn|income|billion|million)\\b",
                "\\b(?:credit|loan|debt|consolidation|mortgage|refinance)\\b",
                "\\b(?:viagra|cialis|medication|prescription|pharmacy)\\b",
                "\\b(?:online\\s*marketing|internet\\s*marketing|work\\s*from\\s*home)\\b",
                "\\b(?:buy\\s*direct|buy\\s*now|order\\s*now|compare\\s*prices)\\b",
                "\\b(?:amazing|miracle|incredible|exclusive\\s*deal)\\b",
                "\\b(?:lose\\s*weight|weight\\s*loss|diet|exercise|pounds)\\b",
                "\\b(?:as\\s*seen\\s*on|stop\\s*spam|opt-out)\\b",
                "\\b(?:meet\\s*singles|singles\\s*dating|hot\\s*girls|lonely)\\b",
                "\\b(?:online\\s*pharmacy|cheap\\s*meds|save\\s*big\\s*money)\\b",
                "\\b(?:multi-level\\s*marketing|get\\s*out\\s*of\\s*debt)\\b",
                "\\b(?:lowest\\s*price|price\\s*reduction|buy\\s*direct)\\b",
                "\\b(?:credit\\s*card|no\\s*credit\\s*check|remove\\s*bad\\s*credit)\\b",
                "\\b(?:lose\\s*weight\\s*fast|increased\\s*testosterone)\\b",
                "\\b(?:money\\s*back|100%\\s*satisfaction|cancel\\s*at\\s*any\\s*time)\\b",
                "\\b(?:this\\s*is\\s*not\\s*a\\s*scam|free\\s*investment)\\b",
                "\\b(?:meet\\s*girls|teenagers|online\\s*degree|earn\\s*extra\\s*cash)\\b",
                "\\b(?:earn\\s*money|miracle)\\b",
                "\\b(?:no\\s*catch|no\\s*obligation|no\\s*hidden\\s*costs)\\b",
                "\\b(?:apply\\s*now|buy\\s*direct|get\\s*it\\s*now|do\\s*it\\s*today)\\b",
                "\\b(?:while\\s*supplies\\s*last|dear\\s*friend|st\\s*month\\s*free)\\b",
                "\\b(?:reverses\\s*age|double\\s*your\\s*income|as\\s*seen\\s*on\\s*TV)\\b",
                "\\b(?:increase\\s*sales|increase\\s*traffic)\\b",
                "\\b(?:cancel\\s*at\\s*any\\s*time|see\\s*for\\s*yourself)\\b",
                "(?:(save|Save)\\s+\\d+%)",
                "(?:save\\s+(on|big|up|\\$))"
            ]
        }
    ]
}